    <section class="update">
            <h1>Update Su kien </h1>
            <select ng-model="nha">
                <option>Nhà C1</option>
                <option>Nhà C2</option>
                <option>Thư viện Tạ Quang Bửu</option>
            </select>
            <input type="button" name=""  ng-click= "chonNha()" value = "Submit">
          
          <div class="form-group" id="">
            <ul>
              <li ng-repeat="sukien in sukiens">
                  <div class="" id="">
                      <div class="">
                          <label>Dia diem: {{sukien.diaDiem}}</label>
                          
                      </div>
                      <div class="">
                          <label>Hinh anh: {{sukien.hinhAnh}}</label>
                      </div>
                      <div class="">
                          <label>Link: {{sukien.link}}</label>
                      </div>
                      <div class="">
                          <label>Time: {{sukien.time}}</label>
                      </div>
                      <div class="">
                          <label>Title: {{sukien.title}}</label>
                      </div>
                </div>    
                <!-- <em>{{sukiens}}</em> -->
                <input type="button" class="btn btn-danger" name="" ng-click= "xoa()" value = "Delete">
                <hr>
              </li>
            </ul>
          </div>

          <div class="sukienmoi" id="">
                <div class="form-group">
                    <label>Dia diem</label>
                    <input type="text" ng-model="diaDiem" placeholder="Dia diem">
                </div>
                <div class="form-group">
                    <label>Hinh anh</label>
                    <input type="text" ng-model="hinhAnh" placeholder="Hinh anh">
                </div>
                <div class="form-group">
                    <label>Link</label>
                    <input type="text" ng-model="link" placeholder="Link">
                </div>
                <div class="form-group">
                    <label>Time</label>
                    <input type="text" ng-model="time" placeholder="time">
                </div>
                <div class="form-group">
                    <label>Title</label>
                    <input type="text" ng-model="title"  placeholder="title">
                </div>
            <input type="button" name="" ng-click= "addSukien()" value = "Submit">
          </div>    
